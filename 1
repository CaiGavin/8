#coding:utf-8ABCDEFGimport TkinterABCDEFGimport tkFileDialogABCDEFGimport reABCDEFGimport matplotlib.pyplot as pltABCDEFGimport tkFontABCDEFGimport osABCDEFGimport pandas as pdABCDEFGimport xlsxwriterABCDEFGimport tkMessageBox as msgABCDEFGfrom matplotlib.ticker import MultipleLocator, FormatStrFormatterABCDEFGimport pdbABCDEFGABCDEFG#import sysABCDEFG#reload(sys)ABCDEFG#sys.setdefaultencoding('utf-8')ABCDEFGABCDEFGfileType=[('','.xlsm'),('','.csv'),('','.xlsx'),('','.xls')]ABCDEFGABCDEFGdef getFolderPath():ABCDEFG    global folderpathABCDEFG    folderpath=tkFileDialog.askdirectory(initialdir=os.getcwd())ABCDEFG    #print os.getcwd()ABCDEFG    return(folderpath)ABCDEFGdef getFilePath():ABCDEFG    #x=getFolderPath()ABCDEFG    #print xABCDEFG    filepath=tkFileDialog.askopenfilename(initialdir=getFolderPath(),title="选取文件",filetypes=fileType)ABCDEFG    #root.destroy() #root.quit()ABCDEFG    return(filepath)  ABCDEFGdef importDataFile():ABCDEFG    exlpath=getFilePath()ABCDEFG    global dfABCDEFG    df=pd.read_excel(exlpath,sheetname=0)ABCDEFG    eparam_c=re.compile('^\w+\(.*\)',re.IGNORECASE)ABCDEFG    etest_tuple=('cgg_0v','cgg_inv','cgg_acc','cgc_0v','cgc_inv','cgc_acc','vt_gm','vt_lin','vt_sat','idlin','idsat','ideff',ABCDEFG                'ioff','isoff','igoff','iboff','ig_inv','id1','id2','dibl','swing_lin','swing_sat','body_sat','gm','rout')ABCDEFG    for eachcolumn in df.columns:ABCDEFG        eparam_m=eparam_c.match(eachcolumn)ABCDEFG        if df[eachcolumn].count()==0:ABCDEFG            #删除空列ABCDEFG            df.drop(eachcolumn,axis=1, inplace=True)#axis参数告诉函数舍弃列还是行。axis=0舍弃行。ABCDEFG        elif eparam_m or eachcolumn.lower() in etest_tuple:ABCDEFG            ltb6.insert(Tkinter.END,str(eachcolumn))ABCDEFG        else:ABCDEFG            ltb.insert(Tkinter.END,str(eachcolumn))ABCDEFGdef selectLtbIndex(selecttoLtb):ABCDEFG    listbox_tuple=(ltb,ltb1,ltb2,ltb3,ltb4,ltb5,ltb6)ABCDEFG    for eachltb in  listbox_tuple:ABCDEFG        select_list=[]ABCDEFG        select_list=list(eachltb.curselection())ABCDEFG        for eachselect in eachltb.curselection():ABCDEFG            if eachltb.get(eachselect) not in selecttoLtb.get(0,selecttoLtb.size()-1):ABCDEFG                selecttoLtb.insert(Tkinter.END,eachltb.get(eachselect))ABCDEFG            else:ABCDEFG                select_list.remove(eachselect)ABCDEFG                msg.showinfo('Attention:',eachltb.get(eachselect)+' is alreay selected in')ABCDEFG        if len(select_list)>0:ABCDEFG            select_list.reverse()ABCDEFG            for each_item in select_list:ABCDEFG                eachltb.delete(each_item)ABCDEFG                ABCDEFG#df[(df['a'] > 0) & (df['b'] < 0) | df['c'] > 0] 行条件筛选ABCDEFGgrp_tuple=('k1','k2','k3','k4','k5','k6','k7','k8','k9')ABCDEFGABCDEFGdef getDfmGrp(InDfm,LtBxLst,funcSendDfm,FlNm='',PartFigNm='',EtstNm=''):ABCDEFG    grpby=list(LtBxLst)ABCDEFG    Lenlist=len(LtBxLst)ABCDEFG    grp=InDfm.groupby(by=grpby,sort=False)ABCDEFG    #pdb.set_trace()ABCDEFG    key_tuple=grp_tuple[:Lenlist]ABCDEFG    for key_tuple,group in grp:ABCDEFG        nameStr=''ABCDEFG        if Lenlist>1:ABCDEFG            for ky in key_tuple:ABCDEFG                nameStr=nameStr+'_'+ str(ky)ABCDEFG        else:ABCDEFG            nameStr=nameStr+'_'+ str(key_tuple)ABCDEFG        ABCDEFG        if nameStr.startswith('_'):ABCDEFG            nameStr=nameStr[1:]ABCDEFG        if nameStr.find('_ckt'):ABCDEFG            nameStr=nameStr.replace('_ckt','')ABCDEFG        funcSendDfm(group,nameStr,FlNm,PartFigNm,EtstNm)ABCDEFG        ABCDEFGdef getMultiFileData(btn):ABCDEFG    #首先获取TrendBy data，以便后续调用，且限制trendby为唯一参数ABCDEFG    global trendParam,btnNameABCDEFG    btnName=str(btn['text'])ABCDEFG    if ltb4.size()==1:ABCDEFG        trendParam=ltb4.get(0)ABCDEFG    elif ltb4.size()==0:ABCDEFG        msg.showinfo('Attention:','Please select a TrendBy Parameter!')ABCDEFG    elif ltb4.size()>1:ABCDEFG        msg.showinfo('Attention:','Only ONE TrendBy Parameter is allowed!')ABCDEFG    #当前目