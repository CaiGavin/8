FG    if ltb3.size()>0:ABCDEFG        lst3=ltb3.get(0,ltb3.size()-1)ABCDEFG        print lst3ABCDEFG        getDfmGrp(df_MultiEtest,lst3,drawPlot,fileName,strpartFigNm,EtstNm=EtestNm)ABCDEFG#        if df3_ltb1.empty==False:ABCDEFG#            lblnm=str(eachltb3_item1_element)ABCDEFG#            drawPlot(df3_ltb1[[trendParam,EtestNm]],trendParam,EtestNm,lblnm)ABCDEFG    else:ABCDEFG        msg.showinfo('Error:','No Item in 【MultiPlotBy】 Box!')ABCDEFG    ABCDEFG    if btnName=='GenImageReport':ABCDEFG        NoInFileNameStr_tuple=('/','*','\\','?','<','>',':','"','|',':')ABCDEFG        image_name=fileName+'_'+strpartFigNm+'_'+ str(EtestNm)+'.jpg'ABCDEFG        for eachstr in NoInFileNameStr_tuple:ABCDEFG            if eachstr in image_name:ABCDEFG                image_name=image_name.replace(eachstr,'')ABCDEFG                ABCDEFG        xmajorFormatter = FormatStrFormatter('%5.2f') #设置x轴标签文本的格式ABCDEFG        figure_name=strpartFigNm+'_'+ str(EtestNm)ABCDEFG        box=fig.get_position()ABCDEFG        fig.set_position([box.x0,box.y0,box.width*0.8,box.height])ABCDEFG        plt.title(figure_name,fontsize=16,family="Arial",weight='bold')ABCDEFG        plt.xlabel('L',fontsize=13,family="Arial",weight='bold')ABCDEFG        plt.ylabel(str(EtestNm),fontsize=13,family="Arial",weight='bold')ABCDEFG        plt.xticks(fontsize=12,weight='bold')ABCDEFG        plt.yticks(fontsize=12,weight='bold')ABCDEFG        if trendParam in ('L','NFIN'):ABCDEFG            plt.xscale('log')ABCDEFG        fig.xaxis.set_major_formatter(xmajorFormatter)ABCDEFG        fig.grid(color='#95a5a6',linestyle='--', linewidth=1 ,axis='both',alpha=0.4)ABCDEFG        fig.legend(loc='center left',bbox_to_anchor=(1, 0.5),ncol=1,fontsize=10)ABCDEFG        fig.spines['bottom'].set_linewidth(2)ABCDEFG        fig.spines['top'].set_linewidth(2)ABCDEFG        fig.spines['left'].set_linewidth(2)ABCDEFG        fig.spines['right'].set_linewidth(2)ABCDEFG        plt.savefig(image_name,dpi=400)ABCDEFG        plt.show()ABCDEFG        sheet.insert_image(row,col,image_name,{'x_scale':0.185,'y_scale':0.22})#,{'x_scale':0.3,'y_scale':0.4}ABCDEFG    col+=7ABCDEFG                ABCDEFGdef drawPlot(plotDataDfm,LabelNm,fileName,strpartFigNm,EtestNm,*NoNeed):ABCDEFG    ABCDEFG    print plotDataDfmABCDEFG    xcolNm=trendParamABCDEFG    ycolNm=EtestNmABCDEFG    #两列的表示方法为df[['NFIN','W']]ABCDEFG    df_plotData=plotDataDfm[[xcolNm,ycolNm]]ABCDEFG    ABCDEFG    df_plotData=df_plotData.fillna(' ')ABCDEFG    x=df_plotData[xcolNm].valuesABCDEFG    y=df_plotData[ycolNm].valuesABCDEFG    #print df_plotDataABCDEFG    ABCDEFG    if btnName=='GenImageReport':ABCDEFG        x=df_plotData[xcolNm].valuesABCDEFG        y=df_plotData[ycolNm].valuesABCDEFG        if 'sim' in LabelNm:ABCDEFG            plt.plot(x,y,'p-',label=str(LabelNm),color='black',markersize=7,linewidth = '1',)ABCDEFG        elif 'target' in LabelNm.lower():ABCDEFG            plt.plot(x,y,'s',label=str(LabelNm),markersize=7)ABCDEFG        elif 'model' in LabelNm:ABCDEFG            plt.plot(x,y,'p-',label=str(LabelNm),color='blue',markersize=7,linewidth = '1',)ABCDEFG        else:ABCDEFG            plt.plot(x,y,'--',label=str(LabelNm),markersize=7)ABCDEFG    elif btnName=='GenExcelReport':ABCDEFG        global col_ExcelReportABCDEFG        global row_ExcelReportABCDEFG        sheet.write(row,col+1,trendParam)ABCDEFG        sheet.write(row,col+2,EtestNm)ABCDEFG        sheet.write(row,col+0,fileName)ABCDEFG        sheet.write(row+1,col+0,strpartFigNm)ABCDEFG        sheet.write(row+2,col+0,LabelNm)ABCDEFG        for rowId,each_Xdata in enumerate(x,0):ABCDEFG            sheet.write(row+rowId+1,col+1,each_Xdata)ABCDEFG            sheet.write(row+rowId+1,col+2,y[rowId])ABCDEFG               ABCDEFGdef delete_file_jpg(file_path):ABCDEFG    for root,dirs,files in os.walk(file_path):ABCDEFG        for name in files:ABCDEFG            if name.endswith('.jpg'):ABCDEFG                os.remove(os.path.join(root,name))ABCDEFGABCDEFGroot=Tkinter.Tk()